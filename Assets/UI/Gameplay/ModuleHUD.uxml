                    <ui:VisualElement name="resources-crystal-container" class="available-resources" style="flex-grow: 0; flex-direction: row; justify-content: center; padding-right: 10px; padding-top: 10px; padding-bottom: 10px;">
                        <ui:VisualElement name="resources-crystal-sprite" style="background-image: url(&apos;project://database/Assets/Sprites/UI/Gameplay/meteor_squareDetailedSmall.png?fileID=2800000&amp;guid=4d2dae0aef8989848ad4777a9b885f3d&amp;type=3#meteor_squareDetailedSmall&apos;); width: 62px; height: auto; -unity-background-scale-mode: scale-and-crop; rotate: -22deg; -unity-background-image-tint-color: rgb(141, 255, 250);" />
