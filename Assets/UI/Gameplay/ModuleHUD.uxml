                    </ui:VisualElement>
                    <ui:VisualElement name="resources-arrow" style="background-image: url(&apos;project://database/Assets/Sprites/UI/Gameplay/right.png?fileID=2800000&amp;guid=aaab75f58cf972b448e28947b25bb9ad&amp;type=3#right&apos;); width: 77px; height: auto; -unity-background-scale-mode: scale-to-fit; visibility: visible; display: flex;" />
                    <ui:VisualElement name="resources-stored-container" style="flex-grow: 0; flex-direction: row; justify-content: center; padding-left: 0; padding-right: 10px; padding-top: 10px; padding-bottom: 10px;">
                        <ui:VisualElement name="resources-stored-sprite" style="width: 50px; height: auto; align-items: center; justify-content: center;">
                            <ui:VisualElement name="resources-stored-sprites" style="background-image: url(&apos;project://database/Assets/Sprites/UI/Gameplay/powerupRed_bolt.png?fileID=2800000&amp;guid=39bbca0c1db9278409b91481f3a783f2&amp;type=3#powerupRed_bolt&apos;); width: 80%; height: 80%; -unity-background-scale-mode: scale-to-fit;" />
                        </ui:VisualElement>
                        <ui:Label text="100/100" display-tooltip-when-elided="true" name="resources-stored-label" class="resources-label" style="width: auto; height: auto;" />
