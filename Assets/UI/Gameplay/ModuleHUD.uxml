        </ui:VisualElement>
        <ui:VisualElement name="module-hud-inactive" style="background-image: none; width: 100%; height: 100%; -unity-slice-left: 25; -unity-slice-top: 25; -unity-slice-right: 25; -unity-slice-bottom: 25; display: flex; justify-content: space-around; align-items: center;">
            <ui:VisualElement name="module-hud-inactive-container" class="glass-background" style="flex-direction: row; flex-grow: 0; height: 66%; width: 100%; padding-left: 15px; padding-right: 15px; padding-top: 15px; padding-bottom: 15px;">
