<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/Gameplay/ModuleHUD.uss?fileID=7433441132597879392&amp;guid=1b4e74142b7cf364aa78c08a789770ee&amp;type=3#ModuleHUD" />
    <ui:VisualElement name="module-hud-root" style="flex-grow: 0; width: 100%; height: 200px; margin-top: 20px;">
        <ui:VisualElement name="module-hud-active" style="width: 100%; height: 100%; visibility: visible; display: none; justify-content: flex-start;">
            <ui:VisualElement name="module-hud-content" class="glass-background" style="justify-content: space-between; padding-left: 15px; padding-right: 15px; padding-top: 15px; padding-bottom: 15px; flex-grow: 1;">
