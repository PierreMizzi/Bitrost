                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="tips-container" style="flex-grow: 1; flex-direction: row; justify-content: flex-start;">
                <ui:Label text="Left Click : Fire" display-tooltip-when-elided="true" name="tips-fire" class="tip-label" />
                <ui:Label text="Right Click : Switch Mode" display-tooltip-when-elided="true" name="tips-switch-mode" class="tip-label" style="-unity-text-align: middle-left;" />
                <ui:Label text="E : Retrieve" display-tooltip-when-elided="true" name="tips-retrieve" class="tip-label" style="-unity-text-align: middle-left;" />
