<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <Style src="project://database/Assets/UI/Gameplay/GameOverPanel.uss?fileID=7433441132597879392&amp;guid=db7829885e5133f4e916ed7b2bda074c&amp;type=3#GameOverPanel" />
    <ui:VisualElement name="pause-panel-root" style="position: absolute; width: 100%; height: 100%; align-items: center; justify-content: center;">
        <ui:VisualElement name="background" style="position: absolute; left: 0; top: 0; right: 0; bottom: 0; background-color: rgba(0, 0, 0, 0.53);" />
        <ui:VisualElement name="pause-panel-container" style="width: 37%; height: 60%; padding-left: 0; padding-right: 0; padding-top: 0; padding-bottom: 0;">
            <ui:VisualElement name="top-part" style="height: 15%; background-color: rgb(34, 127, 195); width: 40%; border-top-left-radius: 20px; border-top-right-radius: 20px; max-width: none;">
                <ui:Label text="PAUSE" display-tooltip-when-elided="true" name="top-label" style="width: auto; height: 100%; -unity-text-align: middle-center; font-size: 43px; color: rgb(255, 255, 255);" />
                <ui:VisualElement name="bolt-container" style="position: absolute; width: 100%; height: 100%; flex-direction: row; justify-content: space-between;">
                    <ui:VisualElement name="bolt-top-left" class="bolt-top" style="background-color: rgb(25, 92, 141);" />
                    <ui:VisualElement name="bolt-top-right" class="bolt-top" style="left: auto; right: 20px; background-color: rgb(25, 92, 141);" />
                </ui:VisualElement>
            </ui:VisualElement>
            <ui:VisualElement name="bottom-part" style="flex-grow: 0; border-bottom-left-radius: 20px; border-bottom-right-radius: 20px; height: 85%; border-top-left-radius: 0; border-top-right-radius: 20px; background-color: rgb(214, 222, 231); flex-direction: row; justify-content: center;">
                <ui:VisualElement name="bolt-container" style="position: absolute; width: 100%; height: 100%; flex-direction: row; justify-content: space-between;">
                    <ui:VisualElement name="bolt-bottom-right" class="bolt-bottom" />
