                    <ui:VisualElement name="bolt-top-right" class="bolt-top" style="left: auto; right: 20px; background-color: rgb(152, 157, 164);" />
                </ui:VisualElement>
                <ui:VisualElement name="button-container" style="width: 64%; justify-content: space-between; align-items: center; height: 100%; margin-top: 0; padding-top: 20%; padding-bottom: 20%;">
                    <ui:Button text="Resume" display-tooltip-when-elided="true" name="resume-button" class="custom-button" />
                    <ui:Button text="Restart" display-tooltip-when-elided="true" name="restart-button" class="custom-button" />
                    <ui:Button text="Menu" display-tooltip-when-elided="true" name="menu-button" class="custom-button" />
