                        <ui:Label text="Drop turrets on asteroids, extract energy from it and defend yourself !" display-tooltip-when-elided="true" name="welcome-text-2" class="content-container-text" />
                        <ui:Label text="Supported controllers are &lt;b&gt;keyboard &amp; mouse&lt;/b&gt; (ZQSD)" display-tooltip-when-elided="true" name="welcome-text-controllers" class="content-container-text" />
                    </ui:VisualElement>
